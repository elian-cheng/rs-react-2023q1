{
  "name": "rs-react-2023q1",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "cross-env NODE_ENV=development ts-node server.ts",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --ssr src/ServerEntry.tsx --outDir dist/server",
    "prod": "npm run build && cross-env NODE_ENV=production ts-node server.ts",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,css,scss}\"",
    "lint": "eslint --fix \"src/**/*.{ts,tsx,js,jsx}\"",
    "test": "vitest run --coverage --silent",
    "nyc": "nyc report --reporter=text-summary",
    "cy": "start-server-and-test start http-get://localhost:3000 cy:open",
    "cy:open": "cypress open --e2e --browser chrome",
    "cy:run": "cypress run --e2e",
    "testing": "start-server-and-test start http://localhost:3000 cy:run"
  },
  "lint-staged": {
    "*.(ts|tsx)": [
      "npm run lint"
    ],
    "*.(ts|tsx|scss|cssjson|md)": [
      "npm run format"
    ]
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/cypress/"
    ]
  },
  "dependencies": {
    "@mdx-js/mdx": "^2.3.0",
    "@mdx-js/react": "^2.3.0",
    "@mdx-js/rollup": "^2.3.0",
    "@reduxjs/toolkit": "^1.9.3",
    "@remix-run/node": "^1.15.0",
    "@remix-run/router": "^1.5.0",
    "axios": "^1.3.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "cross-fetch": "^3.1.5",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.9.0"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.4",
    "@cypress/react18": "^2.0.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/mdx": "^2.0.4",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-istanbul": "^0.29.3",
    "compression": "^1.7.4",
    "cypress": "^12.10.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jsdom": "^21.1.1",
    "msw": "^1.2.1",
    "nyc": "^15.1.0",
    "prettier": "^2.8.4",
    "sass": "^1.59.3",
    "start-server-and-test": "^2.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vite-plugin-istanbul": "^4.0.1",
    "vite-tsconfig-paths": "^4.0.7",
    "vitest": "^0.29.2"
  }
}
